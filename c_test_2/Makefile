final:
	(cd count_bits; make final)
	(cd id; make final)
	(cd invert_bits; make final)
	(cd line_diff; make final)
	(cd merge_ll; make final)
	(cd rev_link; make final)
	(cd right_rotate; make final)
	(cd rm_blank; make final)
	(cd tail; make final)
clean:
	(cd count_bits; make clean)
	(cd id; make clean)
	(cd invert_bits; make clean)
	(cd line_diff; make clean)
	(cd merge_ll; make clean)
	(cd rev_link; make clean)
	(cd right_rotate; make clean)
	(cd rm_blank; make clean)
	(cd tail; make clean)


